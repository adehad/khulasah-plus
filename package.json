{
  "name": "khulasah-plus",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "bun run dev-server",
    "dev-task": "vite",
    "deploy": "npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build",
    "start": "bun run dev",
    "start-remote": "vite --host",
    "lint": "biome lint",
    "format": "biome format --write",
    "generate-pwa-assets": "pwa-assets-generator",
    "preview": "vite preview"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shoelace-style/shoelace": "^2.20.1",
    "@thepassle/app-tools": "^0.9.13",
    "lit": "^3.3.1",
    "urlpattern-polyfill": "^10.1.0",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/node": "^24.3.0",
    "@vite-pwa/assets-generator": "^1.0.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^0.20.5"
  }
}
